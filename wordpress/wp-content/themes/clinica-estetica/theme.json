				}
			},
			"h4": {
				"typography": {
					"fontSize": "clamp(1.56rem, 0.77vw + 1.37rem, 2.11rem)",
					"lineHeight": "1.2"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "clamp(1.25rem, 0.47vw + 1.13rem, 1.58rem)",
