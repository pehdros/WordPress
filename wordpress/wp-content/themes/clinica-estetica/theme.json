				}
			},
			"heading": {
				"color": {
					"text": "#61764b"
				},
				"typography": {
					"fontFamily": "var:preset|font-family|poppins-medium",
					"textTransform": "none"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var:preset|color|custom-tg-white"
