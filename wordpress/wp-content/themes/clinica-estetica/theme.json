{
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#61764b",
					"name": "TG-Sage ",
					"slug": "custom-tg-sage"
				},
				{
					"color": "#fad6a5",
					"name": "TG-Bege",
					"slug": "custom-tg-bege"
				},
				{
					"color": "#9ba17b",
					"name": "TG-SageLight",
					"slug": "custom-tg-sage-light"
				},
				{
					"color": "#cfb997",
					"name": "TG-BrownLight",
					"slug": "custom-tg-brown-light"
				},
				{
					"color": "#f3f3f3",
					"name": "TG-White",
					"slug": "custom-tg-white"
				},
				{
					"color": "#030303",
					"name": "TG-Black",
					"slug": "custom-tg-black"
				}
			]
		},
		"layout": {
			"contentSize": "1544px",
			"wideSize": "1544px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Poppins Medium",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/poppins-medium_normal_500.ttf"
							]
						}
					],
					"fontFamily": "Poppins Medium",
					"slug": "poppins-medium"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Poppins",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/poppins_normal_700.ttf"
							]
						},
						{
							"fontFamily": "Poppins",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/poppins_normal_400.ttf"
							]
						}
					],
					"fontFamily": "Poppins",
					"slug": "poppins"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/paragraph": {
				"typography": {
					"lineHeight": "1.6"
				}
			}
		},
		"color": {
			"background": "var:preset|color|custom-tg-white",
			"text": "var:preset|color|custom-tg-sage"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var:preset|color|custom-tg-bege",
					"text": "var:preset|color|custom-tg-sage"
				},
				"typography": {
					"fontFamily": "var:preset|font-family|poppins",
					"fontSize": "0.9rem"
				}
			},
			"heading": {
				"color": {
					"text": "#61764b"
				},
				"typography": {
					"fontFamily": "var:preset|font-family|poppins-medium",
					"textTransform": "none"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var:preset|color|custom-tg-brown-light"
					}
				},
				"color": {
					"text": "var:preset|color|custom-tg-sage"
				},
				"typography": {
					"fontFamily": "var:preset|font-family|poppins",
					"textDecoration": "underline"
				}
			}
		},
		"typography": {
			"fontFamily": "var:preset|font-family|poppins"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2,
	"$schema": "https://schemas.wp.org/wp/6.2/theme.json"
}